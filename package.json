{
  "name": "fuzzybase",
  "productName": "Fuzzybase",
  "version": "0.0.1",
  "description": "Fuzzybase - SQL database client with fuzzy queries support",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krzysztof-miemiec/fuzzybase.git"
  },
  "author": {
    "name": "Krzysztof Miemiec",
    "email": "krzysztof.miemiec@gmail.com",
    "url": "https://linkedin.com/in/krzysztof-miemiec"
  },
  "license": "MIT",
  "homepage": "https://github.com/krzysztof-miemiec/fuzzybase#readme",
  "bugs": {
    "url": "https://github.com/krzysztof-miemiec/fuzzybase/issues"
  },
  "keywords": [
    "postgres",
    "electron",
    "react",
    "flux",
    "webpack"
  ],
  "devEngines": {
    "node": ">=8.x",
    "npm": ">=5.x"
  },
  "main": "./dist/main/main.js",
  "scripts": {
    "postinstall": "node ./scripts/patch.js && electron-builder install-app-deps",
    "purge": "yarn clean && rimraf 'node_modules' && yarn",
    "clean": "rimraf '{release,dist,.tscache,reports,screenshots}'",
    "extension": "rimraf '{extension/**/*.so, extension/**/*.o, extension/fuzzy--*.sql}' && zip -r ./static/fuzzy.zip ./extension",
    "build:prod": "electron-webpack && yarn build:sql",
    "build:release": "electron-builder -mwl",
    "build:sql": "(cd ./src/sql && make everything)",
    "start:dev": "electron-webpack dev",
    "start:prod": "yarn build:prod && yarn build:release",
    "lint": "mkdir -p reports && tslint './src/**/*.{js,ts,tsx}' --project ./ --format json --out './reports/lint.json' || tslint './src/**/*.{js,ts,tsx}' --project ./ --format stylish"
  },
  "electronWebpack": {
    "main": {
      "webpackConfig": "webpack.main.additions.js"
    },
    "renderer": {
      "webpackConfig": "webpack.renderer.additions.js",
      "webpackDllConfig": "webpack.main.additions.js"
    }
  },
  "build": {
    "productName": "Fuzzybase",
    "appId": "com.krzysztofmiemiec.fuzzybase",
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 150,
          "type": "file"
        },
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "files": [
      "dist"
    ],
    "directories": {
      "buildResources": "dist",
      "output": "release"
    },
    "mac": {
      "target": [
        "dir",
        "dmg"
      ],
      "icon": "src/assets/icons/icon.icns"
    },
    "win": {
      "target": [
        "dir",
        "portable",
        "nsis"
      ],
      "icon": "src/assets/icons/icon.ico"
    },
    "linux": {
      "target": [
        "dir",
        "deb"
      ],
      "icon": "src/assets/icons"
    }
  },
  "dependencies": {
    "@material-ui/core": "3.1.0",
    "@material-ui/icons": "3.0.1",
    "@types/react-data-grid": "^4.0.0",
    "ag-grid-community": "^19.0.0",
    "ag-grid-react": "^19.0.0",
    "classnames": "2.2.6",
    "electron-debug": "2.0.0",
    "electron-devtools-installer": "2.2.4",
    "history": "4.7.2",
    "i18next": "11.9.0",
    "i18next-electron-language-detector": "0.0.10",
    "lodash": "4.17.11",
    "monaco-editor": "^0.14.3",
    "pg": "7.4.3",
    "raven-js": "3.27.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-hot-loader": "4.3.11",
    "react-i18next": "7.12.0",
    "react-jss": "^8.6.1",
    "react-redux": "5.0.7",
    "react-resize-detector": "^3.1.3",
    "react-router": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "4.0.0",
    "redux-devtools-extension": "2.13.5",
    "redux-observable": "1.0.0",
    "redux-persist": "5.10.0",
    "rxjs": "6.3.2",
    "source-map-support": "0.5.9",
    "sudo-prompt": "^8.2.3",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@h1/lemmy": "0.3.2",
    "@types/classnames": "2.2.6",
    "@types/codemirror": "0.0.60",
    "@types/history": "4.7.0",
    "@types/i18next": "11.9.0",
    "@types/jest": "23.3.2",
    "@types/jss": "^9.5.6",
    "@types/lodash": "^4.14.116",
    "@types/pg": "7.4.10",
    "@types/react": "16.4.14",
    "@types/react-redux": "6.0.9",
    "@types/react-router": "4.0.31",
    "@types/react-router-redux": "5.0.16",
    "@types/uuid": "3.4.4",
    "cpx": "1.5.0",
    "electron": "3.0.3",
    "electron-builder": "20.28.4",
    "electron-webpack": "2.3.1",
    "electron-webpack-ts": "2.1.1",
    "jest": "23.6.0",
    "monaco-editor-webpack-plugin": "^1.5.4",
    "node-sass": "^4.9.3",
    "rimraf": "2.6.2",
    "sass-loader": "^7.1.0",
    "tslint": "5.11.0",
    "tslint-loader": "3.6.0",
    "tslint-react": "3.6.0",
    "typescript": "3.0.3",
    "webpack": "4.20.2",
    "webpack-build-notifier": "0.1.30"
  },
  "resolutions": {
    "webpack-sources": "1.3.0"
  }
}
