{
  "name": "fuzzybase",
  "productName": "Fuzzybase",
  "version": "0.0.1",
  "description": "Fuzzybase - SQL database client with fuzzy queries support",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krzysztof-miemiec/fuzzybase.git"
  },
  "author": {
    "name": "Krzysztof Miemiec",
    "email": "krzysztof.miemiec@gmail.com",
    "url": "https://linkedin.com/in/krzysztof-miemiec"
  },
  "license": "MIT",
  "homepage": "https://github.com/krzysztof-miemiec/fuzzybase#readme",
  "bugs": {
    "url": "https://github.com/krzysztof-miemiec/fuzzybase/issues"
  },
  "keywords": [
    "postgres",
    "electron",
    "react",
    "flux",
    "webpack"
  ],
  "devEngines": {
    "node": ">=8.x",
    "npm": ">=5.x"
  },
  "main": "./app/main.js",
  "scripts": {
    "postinstall": "cd app && yarn install",
    "clean": "rimraf ./release && rimraf ./app/dist",
    "lint": "mkdir -p reports && tslint './app/src/**/*.{js,ts,tsx}' --project ./ --format json --out './reports/lint.json' || tslint './app/src/**/*.{js,ts,tsx}' --project ./ --format stylish",
    "test": "cross-env NODE_ENV=test node --trace-warnings ./test/runTests.js",
    "test:all": "yarn test && yarn build && yarn test:e2e",
    "test:watch": "yarn test --watch",
    "test:e2e": "cross-env NODE_ENV=test node --trace-warnings ./test/runTests.js e2e",
    "build:main": "cross-env NODE_ENV=production webpack-cli --config webpack.config.electron.js --progress --profile --colors",
    "build:renderer": "cross-env NODE_ENV=production webpack-cli --config webpack.config.app.js --progress --profile --colors",
    "build": "yarn build:main && yarn build:renderer",
    "start:hot": "cross-env HOT=1 NODE_ENV=development electron ./app/main",
    "development": "cross-env NODE_ENV=development node --max_old_space_size=2096 server.js --start-hot",
    "production": "yarn clean && yarn build && build -mwl"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/node_modules"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test/preprocessor.js"
    },
    "testMatch": [
      "**/?(*.)(spec|test).ts?(x)"
    ]
  },
  "build": {
    "productName": "Fuzzybase",
    "appId": "com.krzysztofmiemiec.fuzzybase",
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 150,
          "type": "file"
        },
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "files": [
      "build/dist/",
      "node_modules/",
      "app.html",
      "main.js",
      "main.js.map",
      "app/package.json"
    ],
    "directories": {
      "buildResources": "app/dist",
      "output": "release"
    },
    "mac": {
      "target": [
        "dir"
      ]
    },
    "win": {
      "target": [
        "dir"
      ]
    },
    "linux": {
      "target": [
        "dir",
        "deb"
      ]
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "dependencies": {
    "cross-spawn": "^5.1.0",
    "history": "^4.6.1",
    "react": "^16.0.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.1",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/history": "^4.6.2",
    "@types/jest": "^22.2.2",
    "@types/node": "^9.6.1",
    "@types/react": "^16.1.0",
    "@types/react-dom": "^16.0.4",
    "@types/react-hot-loader": "^3.0.6",
    "@types/react-redux": "^5.0.15",
    "@types/react-router": "^4.0.23",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-router-redux": "^5.0.13",
    "@types/redux-logger": "^3.0.5",
    "@types/sinon": "^4.3.0",
    "asar": "^0.14.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "css-modules-require-hook": "^4.2.3",
    "devtron": "^1.4.0",
    "electron": "2.0.0-beta.6",
    "electron-builder": "^20.8.1",
    "electron-builder-http": "^19.27.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.2.0",
    "jest": "^22.4.3",
    "json-loader": "^0.5.7",
    "lemmy": "^0.0.11",
    "minimist": "^1.2.0",
    "react-test-renderer": "^16.2.0",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.2",
    "sinon": "^4.5.0",
    "spectron": "^3.8.0",
    "style-loader": "^0.20.3",
    "ts-loader": "^4.1.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-middleware": "^3.1.0",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-merge": "^4.1.2"
  }
}
